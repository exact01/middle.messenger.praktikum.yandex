    "@typescript-eslint/parser": "^5.39.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "parcel": "2.7.0",
    "parcel-plugin-handlebars-precompile": "^1.0.2",
    "postcss-import": "^12.0.1",
    "typescript": "^4.8.3"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
